<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>apoiodesk</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>br.com.apoiodesk.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletLogin</display-name>
    <servlet-name>ServletLogin</servlet-name>
    <servlet-class>br.com.apoiodesk.servlets.ServletLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletLogin</servlet-name>
    <url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>FilterAutenticacao</display-name>
    <filter-name>FilterAutenticacao</filter-name>
    <filter-class>br.com.apoiodesk.filtros.FilterAutenticacao</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterAutenticacao</filter-name>
    <url-pattern>/FilterAutenticacao</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>RelatorioServlet</display-name>
    <servlet-name>RelatorioServlet</servlet-name>
    <servlet-class>br.com.apoiodesk.servlets.RelatorioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RelatorioServlet</servlet-name>
    <url-pattern>/RelatorioServlet</url-pattern>
  </servlet-mapping>
</web-app>